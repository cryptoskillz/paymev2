---
layout: layout.njk
permalink: /{{env.LEVEL1NAME}}//{{env.LEVEL2NAME}}s/
---
<!--
This is the top level nav page and it has a js file of the same name in the _includes directory. It uses dataname to generate the nav
-->
{% set theJs %}
    {% include 'level2index.js' %}
{% endset %}
<!-- Page Heading -->
 <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">{{env.LEVEL2NAME | capitalize }}s</h1>
   <!--
    <a href="/{{env.LEVEL1NAME}}/{{env.LEVEL2NAME}}s/new/" class="d-sm-inline-block btn btn-sm btn-primary shadow-sm" id="btn-create-cy">

   
    <i class="fas fa-download fa-sm text-white-50"></i> Create New</a>
    <a href="/{{env.LEVEL1NAME}}s/" class="d-sm-inline-block btn btn-sm btn-primary shadow-sm">
    <i class="fas fa-backward fa-sm text-white-50"></i> Back</a>
    -->
     <div class="mb-3">
    <label for="pageActionSelect">Actions</label>
        <select class="form-control" id="pageActionSelect">
            <option value="0">Select</option>
            <option  value="1">Create new page</option>
            <option  value="2">Back</option>
        </select>
        </div>
</div>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary" id="siteheader">Current {{env.LEVEL2NAME }}s for </h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>{{env.LEVEL2NAME | capitalize}} Name</th>
                        <th>Date Created</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Id</th>
                        <th>{{env.LEVEL2NAME | capitalize}} Name</th>
                        <th>Date Created</th>
                        <th>Actions</th>
                    </tr>
                </tfoot>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>
{% if env.ENVIRONMENT == 'local' %}
    <script type="text/javascript">{{theJs | safe}}</script>
{% else %}
    <script type="text/javascript">{{theJs | jsmin | safe}}</script>
{% endif %}


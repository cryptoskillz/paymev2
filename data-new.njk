---
layout: layout.njk
permalink: /data/new/
---
{% set theJs %}
    {% include 'data-new.js' %}
{% endset %}
<!-- Page Heading -->
   <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800" id="data-header">Create a new {{env.DATAMAIN}}</h1>
        <a href="/{{env.DATAMAIN}}/" class="d-sm-inline-block btn btn-sm btn-primary shadow-sm">
        <i class="fas fa-backward fa-sm text-white-50"></i> Back</a>
    </div>
<p class="mb-4">
</p>

    <div id="formInputs">
      
    </div>
<button class="btn btn-primary btn-user btn-block" id="btn-create">
    Create
</button>
{% if env.ENVIRONMENT == 'local' %}
    <script type="text/javascript">{{theJs | safe}}</script>
{% else %}
    <script type="text/javascript">{{theJs | jsmin | safe}}</script>
{% endif %}
